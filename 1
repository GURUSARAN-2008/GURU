#include <iostream>

int main() {
    int num1, num2, sum; // Declare integer variables
    std::cout << "Enter first number: ";
    std::cin >> num1; // Get input for num1
    std::cout << "Enter second number: ";
    std::cin >> num2; // Get input for num2
    sum = num1 + num2; // Calculate the sum
    std::cout << "Sum: " << sum << std::endl; // Display the sum
    return 0;
}
